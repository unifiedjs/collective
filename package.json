    "Titus Wormer <tituswormer@gmail.com> (wooorm.com)"
  ],
  "description": "Collective governance for unified",
  "devDependencies": {
    "@types/mdast": "^4.0.0",
    "mdast-comment-marker": "^3.0.0",
    "mdast-zone": "^6.0.0",
    "prettier": "^3.0.0",
    "remark-cli": "^12.0.0",
    "remark-preset-wooorm": "^10.0.0",
    "typescript": "^5.0.0",
    "xo": "^0.59.0",
    "yaml": "^2.0.0"
  },
  "keywords": [],
  "license": "CC-BY-4.0",
  "name": "collective",
  "prettier": {
    "bracketSpacing": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm",
      "./script/list-of-humans.js"
    ]
  },
  "repository": "unifiedjs/collective",
  "scripts": {
    "build": "tsc --build --clean && tsc --build",
    "format": "remark --frail --quiet --output -- . && prettier --log-level warn --write -- . && xo --fix",
    "test": "npm run build && npm run format"
  },
  "type": "module",
  "version": "0.0.0",
  "xo": {
    "prettier": true
  }
}
